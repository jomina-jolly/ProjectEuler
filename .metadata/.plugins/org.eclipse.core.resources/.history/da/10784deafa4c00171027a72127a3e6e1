def sum_even_fibonacci(n):
    f=[1,2]
    i=0
    while f[i]<n:
        i+=1
        if i<=1:
            continue
            
        else:
            f[i]=f[i-1]+f[i-2]
            print f
        
    return f 

print sum_even_fibonacci(15)