def prime_numbers(n):
    p=[]
    for i in range(2,n):
        if is_prime(i):
            print i
            print is_prime(i)
            p.append(i)
    return p   
            
def is_prime(n):
    count=0
    if n==0 or n==1:
        return False
    
    else:
        for i in range(2,n/2+1):
            print "n/2=",n/2
            if (n/2)%i==0:
                count+=1
                break
        if count!=0:
            print 'count for ',n,count
            return False
        else:
            return True
        
print prime_numbers(20)
print is_prime(7)


