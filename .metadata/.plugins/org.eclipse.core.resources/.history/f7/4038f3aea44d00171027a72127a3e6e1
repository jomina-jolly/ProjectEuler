#to check a number for prime or not            
def is_prime(n):
    count=0
    if n==0 or n==1:
        return False
    else:
        for i in range(2,n):
            if n%i==0:
                count+=1
        if count>0:
            return False
            
        else:
            return True
        
max_index=10
index=1
number=1
while index<max_index:
    if is_prime(number):
        index+=1
        print 'the prime number of index & number',index,number
        
real_value= 7927
print '10001st prime number is',number  