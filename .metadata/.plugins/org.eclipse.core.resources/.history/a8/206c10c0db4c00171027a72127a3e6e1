def sum_of_multiples(limit):
    result=0
    for n in range(limit):
        if n!=1:    
            if (n%3==0 or n%5==0):
                result+=n
        else:
            result+=0
    return result 

value= raw_input("Enter the max range:")
print sum_of_multiples(value)     

    